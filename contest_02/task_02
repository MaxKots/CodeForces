#include <iostream>
using namespace std;

int main() {
    int n, k;
    cin >> n >> k;

    for (int i = 0; i < k; ++i) {
        if (n % 10 != 0) {
            n--; // минус 1, если последняя цифра != 0
        } else {
            n /= 10; // Делим на 10, если последняя цифра != 0
        }
    }

    cout << n << endl;

    return 0;
}