#include <iostream>
using namespace std;

int main() {
    int x;
    cin >> x;

    // Минимальное кол-во шагов
    int steps = x / 5; // Целая часть от деления на 5
    if (x % 5 != 0) {  // Если есть остаток, добавляем еще шаг
        steps++;
    }

    cout << steps << endl;

    return 0;
}